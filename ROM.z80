	or %11000000
	ld [rNR34],a
	ld a,$80
	ld [rNR30],a
	ld a,$df
	ld [rNR31],a
	ld a,$63
	ld [rNR32],a
	.noch1
	ld a,[NR23]
	cp a,00
	jp z,.noch2
	ld [rNR23],a
