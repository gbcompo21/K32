code_begins:
	ld [system],a
	di	
	ld	SP, $ffff	; set stack to top of HRAM
	dma_Copy2HRAM	; sets up routine from dma.inc that updates sprites
ld	a, IEF_VBLANK	
	ld	[rIE], a	; Set only Vblank interrupt flag
	ei	
			
    ld a, %11100100;set the dmg palettes
	ld [rBGP], a
	ld a, %11010010
	ld [rOBP0], a
	ld a, %00011011
	ld [rOBP1], a

  ld	a, [rLCDC]	; fetch LCD Config. (Each bit is a flag)
	and %11101111   ; use tiles from $9000 for BKG 
	or LCDCF_WIN9C00 | LCDCF_WINON;enable window
	or	LCDCF_OBJON | LCDCF_OBJ8	; enable 8bit wide sprites
	ld	[rLCDC], a	; save LCD Config. Sprites are now visible. 
	ld hl,titlepalettes
	call rgb_SetAllBGP
	ld hl,objpalettes
	call rgb_SetAllOBP
	ld a,3
	ld [bombcount],a
	ld a,104
	ld [rWY],a
	ld a, 7
	ld [rWX],a

	ld a,$77
	ld [rNR50],a
	ld a,$FF
	ld [rNR51],a
	ld a,$80
	ld [rNR52],a
	call	lcd_Stop
	ld	hl, maintiles	; ROM address where we insert  tiles
	ld	de, _VRAM	
	ld	bc, maintilesend - maintiles
	call	mem_Copy

	ld	hl, uitiles	; ROM address where we insert  tiles
	ld	de, $8930
	ld	bc, uitilesend - uitiles
	call	mem_Copy
	ld	hl, titletiles + 2048	; ROM address where we insert  tiles
	ld	de, $8800
	ld	bc, 304
	call	mem_Copy
	ld	hl, titletiles	; ROM address where we insert  tiles
	ld	de, $9000
	ld	bc, titletilesend - titletiles
	call	mem_Copy
	ld a,$FF
	ld [$ff4f],a
	ld	hl, titlecgb	; ROM address where we insert  tiles
	ld	de, $9800
	call	settiles
	ld	hl, titlewincgb	; ROM address where we insert  tiles
	ld	de, $9C00
	call	settiles
	ld a,$00
	ld [$ff4f],a
	ld	hl, titlemap	; ROM address where we insert  tiles
	ld	de, $9800
	call	settiles
	ld	hl, titlewinmap	; ROM address where we insert  tiles
	ld	de, $9C00
	call	settiles
	ld	hl, gamenamemap0	; ROM address where we insert  tiles
	ld	de, $9C0B
	ld	c, 7
	call	mem_Copy
	ld	de, $9C2B
	ld	c, 7
	call	mem_Copy
	ld	de, $9C4B
	ld	c, 7
	call	mem_Copy
	ld	hl, t_superrocket	; ROM address where we insert  tiles
	ld	de, $9c67
	ld	c, 12
	call	mem_Copy
	ld	de, $9c87
	ld	c, 11
