intro::
	ld hl,earthorbitpalettes
	call rgb_SetAllBGP
	call lcd_Stop
	ld a,0
	ld [rWY],a
	ld a,$FF
	ld [$ff4f],a

	ld a,0
	ld	hl, $9800	; ROM address where we insert  tiles
	ld	bc, 1024
	call	mem_Set

	ld a,$00
	ld [$ff4f],a
	ld [invencible],a ; #smort;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;invencible XD
	ld	hl, intromap1	; ROM address where we insert  tiles
	ld	de, $9a40
	call	settiles
	ld	hl, intromap	; ROM address where we insert  tiles
	ld	de, $9800
	call	settiles

	ld	a, [rLCDC]
	or	LCDCF_ON
	ld	[rLCDC], a	; turn LCD back on

	ld	a, [rLCDC]
	and %11011111
	ld	[rLCDC], a	; save LCD Config. Sprites and win are now visible. 

	ld a,20
.loopw
	halt
	nop
	dec a
	jr nz,.loopw

	call pause


	.loopw2
		halt
		nop
		halt
		nop
		ld a,[rSCY]
		inc a
		ld [rSCY],a
		cp 72
		jr nz,.loopw2

	call pause

	ld a,0
	ld [rSCY],a
ret


ending::
	ld hl,earthorbitpalettes
	call rgb_SetAllBGP
	call lcd_Stop
	ld a,$FF
	ld [$ff4f],a

	ld a,$0
	ld	hl, $9800	; ROM address where we insert  tiles
	ld	bc, 1024
	call	mem_Set

	ld a,1
	ld [$99e8],a
	ld [$99e9],a
	ld [$9a08],a
	ld [$9a09],a
	ld a,$00
	ld [$ff4f],a
	ld a,$80
	ld	hl, $9800	; ROM address where we insert  tiles
	ld	bc, 1024
	call	mem_Set
	ld	hl, endingmap	; ROM address where we insert  tiles
	ld	de, $98e0
	call	settiles


	ld hl,earth
	ld de,$8e60
	ld bc, 16+16+16+16
	call mem_Copy



	ld	a, [rLCDC]
	or	LCDCF_ON
	ld	[rLCDC], a	; turn LCD back on

	ld	a, [rLCDC]
	and %11011111
	ld	[rLCDC], a	

	call clearspriteram

	PutSpriteXAddr  ship0,80 - 8
	PutSpriteYAddr  ship0,128 + 8
	sprite_PutFlags ship0,5
	sprite_PutTile  ship0,$e6

	PutSpriteXAddr  ship1,80 - 8
	PutSpriteYAddr  ship1,128 + 16
	sprite_PutFlags ship1,5
	sprite_PutTile  ship1,$e7

	PutSpriteXAddr  ship2,80
	PutSpriteYAddr  ship2,128 + 8
	sprite_PutFlags ship2,5
	sprite_PutTile  ship2,$e8

	PutSpriteXAddr  ship3,80
	PutSpriteYAddr  ship3,128 + 16
	sprite_PutFlags ship3,5
	sprite_PutTile  ship3,$e9

	ld a,20
.loopw
	halt
	nop
	dec a
	jr nz,.loopw

	call pause



ret

