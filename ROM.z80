gameovercheck::;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;gameover;;;;;;;;;terminar

	ld a, [health]
	cp 0
	ret nz
	ld a,$3a
	ld [rNR41],a
	ld a,$a4
	ld [rNR42],a
	ld a,$61
	ld [rNR43],a
	ld a,$80
	ld [rNR44],a
	sprite_PutTile ship0,$14
	sprite_PutTile ship1,$15
	sprite_PutTile ship2,$16
	sprite_PutTile ship3,$17

call part1

	sprite_PutTile ship0,$18
	sprite_PutTile ship1,$19
	sprite_PutTile ship2,$1a
	sprite_PutTile ship3,$1b

call part1
	
	call lcd_Stop

	ld	a, [rLCDC]	
	and %11011101
	ld	[rLCDC], a	; save LCD Config. Sprites and win are not visible. 

	ld a,$FF
	ld [rVRAM_BANK],a
	ld a,$80
	ld	hl, $9800
	ld	bc, uitilesend - uitiles
	call mem_Set
	ld a,0
	ld [rVRAM_BANK],a
	ld a,$80
	ld	hl, $9800
	ld	bc, uitilesend - uitiles
	call mem_Set
	ld	hl, gameover	; ROM address where we insert  tiles
	ld	de, $9906
	ld	bc, gameoverend - gameover
	call	mem_Copy
	ld	a, [rLCDC]
	or	LCDCF_ON
	ld	[rLCDC], a	; turn LCD back on
	ld a,0
	ld [rSCX],a
	.explotar
	halt 
	nop 
	call jpad_GetKeys
	and PADF_A | PADF_START
	jr z,.explotar
jp code_begins

