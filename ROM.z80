lcd_Stop:
    ld	a, [rLCDC]	; LCD-Config
    and	LCDCF_ON	; compare config to lcd-on flag
    ret	z		; return if LCD is already off
  .wait4vblank
    ldh	a, [rLY]   ; ldh is a faster version of ld if in [$FFxx] range
    cp	145  ; are we at line 145 yet?  (finished drawing screen then)
    jr	nz, .wait4vblank
  .stopLCD
    ld	a, [rLCDC]
    xor	LCDCF_ON	; XOR lcd-on bit with lcd control bits. (toggles LCD off)
    ld	[rLCDC], a	; `a` holds result of XOR operation
ret
jpad_GetKeys:
  ; Uses AF, B
  ; get currently pressed keys. Register A will hold keys in the following
  ; order: MSB --> LSB (Most Significant Bit --> Least Significant Bit)
  ; Down, Up, Left, Right, Start, Select, B, A
  ; This works by writing

	; get action buttons: A, B, Start / Select
	ld	a, JOYPAD_BUTTONS; choose bit that'll give us action button info
	ld	[rJOYPAD], a; write to joypad, telling it we'd like button info
	ld	a, [rJOYPAD]; gameboy will write (back in address) joypad info
	ld	a, [rJOYPAD]
	cpl		; take compliment
	and	$0f	; look at first 4 bits only  (lower nibble)
	swap	a	; place lower nibble into upper nibble
	ld	b, a	; store keys in b
	; get directional keys
	ld	a, JOYPAD_ARROWS
	ld	[rJOYPAD], a ; write to joypad, selecting direction keys
	ld	a, [rJOYPAD]
	ld	a, [rJOYPAD]
	ld	a, [rJOYPAD]	; delay to reliablly read keys
	ld	a, [rJOYPAD]	; since we've just swapped from reading
	ld	a, [rJOYPAD]	; buttons to arrow keys
	ld	a, [rJOYPAD]
	cpl			; take compliment
	and	$0f		; keep lower nibble
	or	b		; combine action & direction keys (result in a)
	ld	b, a

	ld	a, JOYPAD_BUTTONS | JOYPAD_ARROWS
	ld	[rJOYPAD], a		; reset joypad

	ld	a, b	; register A holds result. Each bit represents a key
	ld	[jpad],a
ret
updateship:
  	ld a,[jpad]
	and PADF_UP
	jr z,.noup
	GetSpriteYAddr	ship0
	cp 56
	jr c,.noup
   ; move the metasprite
	GetSpriteYAddr ship0  
	sub 2
	PutSpriteYAddr ship0, a
	PutSpriteYAddr ship2, a
	add 8
	PutSpriteYAddr ship1, a
	PutSpriteYAddr ship3, a
	jr .udend
   .noup
  	ld a,[jpad]
	and PADF_DOWN
	jr z,.nodown
	GetSpriteYAddr	ship0
	cp 144
	jr nc,.nodown
   ; move the metasprite
	GetSpriteYAddr ship0  
	add 2
	PutSpriteYAddr ship0, a
	PutSpriteYAddr ship2, a
	add 8
	PutSpriteYAddr ship1, a
	PutSpriteYAddr ship3, a
	jr .udend
	.nodown
	;if not moving then return to main tiles
	ld a,[jpad]
	and %11110000
	jr nz,.ab
	.udend
	sprite_PutTile	ship0,	4;change to main tiles
	sprite_PutTile	ship1,	5
	sprite_PutTile	ship2,	6
	sprite_PutTile	ship3,	7
	.ab 			
	ld a,[jpad]
	and PADF_LEFT
	jp z,.noleft
	sprite_PutTile	ship0,	8;change to breaking animation tiles
	sprite_PutTile	ship1,	9
	sprite_PutTile	ship2,	6
	sprite_PutTile	ship3,	7
	GetSpriteXAddr	ship0
	cp 11
	jr c,.noleft
   ; move the metasprite
	GetSpriteXAddr ship0  
	sub 4
	PutSpriteXAddr ship0, a
	PutSpriteXAddr ship1, a
	add 8
	PutSpriteXAddr ship2, a
	PutSpriteXAddr ship3, a
   .noleft
  	ld a,[jpad]
	and PADF_RIGHT
	jp z,.noright
	sprite_PutTile	ship0,	4;change to main tiles
	sprite_PutTile	ship1,	$78
	sprite_PutTile	ship2,	6
	sprite_PutTile	ship3,	7
	GetSpriteXAddr	ship0
	cp 150
	jr nc,.noright
   ; move the metasprite
	GetSpriteXAddr ship0  
	add 2
	PutSpriteXAddr ship0, a
	PutSpriteXAddr ship1, a
	add 8
	PutSpriteXAddr ship2, a
	PutSpriteXAddr ship3, a
   .noright
	ld a,[pewtimer] 		;get timer for projectiles
	cp 00 				;check if time has passed since last pew(projectile) was fired
	jr nz,.noab			;time not passed then jump to .noa
	ld a,[jpad]
	and PADF_A
	call nz,shoot;call shooting routine if abutton pressed
	ld a,[jpad]
	and PADF_B
	call nz,bomb;call bomb throw routine if b button pressed
	.noab
	ld a,[pewtimer]
	cp 0
	ret z
	dec a
	ld [pewtimer],a
ret
