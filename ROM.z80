checkcollision:: ;check if (x1+4,y1+4) is inside (x,y,x+8,y+8)

	ld a,[x1]
	add 4
	ld b, a
	ld a,[x]
	cp b
	ret nc
	add 8
	cp b
	ret c
	ld a,[y1]
	add 4
	ld b, a
	ld a,[y]
	cp b
	ret nc
	add 8
	cp b
	ret c

	ld a,$F0
ret
checkcollision16::	;check if (x1+4,y1+4) is inside (x,y,x+16,y+16)

	ld a,[x1]
	add 4
	ld b, a
	ld a,[x]
	cp b
	ret nc
	add 16
	cp b
	ret c
	ld a,[y1]
	add 4
	ld b, a
	ld a,[y]
	cp b
	ret nc
	add 16
	cp b
	ret c

	ld a,$F0
ret
